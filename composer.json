{
    "name": "naldi/laravel-sri-hash",
    "description": "SRI Hash generator for laravel",
    "keywords": ["sri", "hash generator", "subresource integrity"],
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Naldi",
            "email": "matheus_naldi@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.2|6.*",
        "illuminate/console": "^5.2|6.*",
        "illuminate/filesystem": "^5.2|6.*"
    },
    "autoload": {
        "psr-4": {
            "Naldi\\LaravelSri\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naldi\\LaravelSri\\SriServiceProvider"
            ],
            "aliases": {
                "Sri": "Naldi\\LaravelSri\\SriFacade"
            }
        }
    }
}
